<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <!-- 数据库配置文件 -->
    <properties resource="application.properties" />


    <context id="mysql" defaultModelType="flat" targetRuntime="MyBatis3Simple">
        <property name="javaFileEncoding" value="UTF-8" />
        <property name="autoDelimitKeywords" value="true" />

        <!--防止产生大量的注释-->
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true" />
        </commentGenerator>



        <!-- 数据库连接 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/help?useSSL=false" userId="root" password="123456"/>

        <!-- 数据库的生成配置 -->
        <javaModelGenerator targetPackage="club.fangqcloud.pojo" targetProject="src/main/java" />
        <sqlMapGenerator targetPackage="mapping" targetProject="src/main/resources" />
        <javaClientGenerator targetPackage="club.fangqcloud.mapper" targetProject="src/main/java" type="XMLMAPPER" />


        <!-- user表 -->
        <table tableName="%">
            <property name="useActualColumnNames" value="true" />
            <!--<generatedKey column="id" sqlStatement="MySql"/>-->
        </table>
    </context>


</generatorConfiguration>